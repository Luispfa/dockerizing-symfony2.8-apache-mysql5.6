                        <li data-i18n="id00419_home:document:rules"></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    
    <div class="page-signin" ng-controller="contactCtrl">
        
        
        <div  class="top-menus">
            <div class="container">



                <div class="breadcrumbsimple">
                    <div class="row">
                        <div class="col-md-12">

                            <ul class="lista">
                                <li class="primero"><a href="http://www.caixabank.com/informacionparaaccionistaseinversores_es.html">Información para accionistas e inversores</a></li>
                                <li><a href="http://www.caixabank.com/informacionparaaccionistaseinversores/gobiernocorporativo_es.html">Espacio del Accionista</a></li>
                                <li><a href="http://www.caixabank.com/espaciodelaccionista/juntageneralaccionistas_es.html">Junta General de Accionistas</a></li>
                                <li><a href="https://www.caixabank.com/informacionparaaccionistaseinversores/gobiernocorporativo/juntageneralaccionistas/2014_es.html">2014</a></li>
                                <li class="ultimo"><a href="#">Votación / Delegación electrónica</a></li>
                            </ul>
                        </div>
                    </div>   
                </div>

                <div class="titulo_subcanales">
                    <div class="row">
                        <div class="col-md-12">
                            <h1>
                                <span class="titulo_secundario">Plataformas Electrónicas</span>
                            </h1>
                        </div>
                    </div> 
                </div>

            </div>
        </div>
        

        <div class="signin-body">

            <div class="container">

                <div class="form-container">

                    <section class="row text-center">
                        <h2 data-i18n="id00063_app:contact:contact">Contactar</h2>
                    </section>


                    <section class="row text-center">
                        <form class="form-horizontal" name="contact_form">
                            <fieldset>

                                <div class="form-group">
                                    <div class="input-group input-group-lg">
                                        <span class="input-group-addon">
                                            <span class="glyphicon glyphicon-envelope"></span>
                                        </span>
                                        <input type="email"
                                               class="form-control"
                                               placeholder="id00065_app:contact:email"
                                               ng-model="mail.from"
                                               data-i18n="i18n-placeholder"
                                               required
                                               name="contact_email"
                                               >
                                    </div>
                                    <span ng-show="contact_form.contact_email.$error.required" data-i18n="id00484_app:contact:obligatorio">Campo obligatorio</span>
                                </div>
                                <div class="form-group">
                                    <div class="input-group input-group-lg">
                                        <span class="input-group-addon">
                                            <span class="glyphicon glyphicon-envelope"></span>
                                        </span>
                                        <input type="email"
                                               class="form-control"
                                               placeholder="id00483_app:contact:repetir-email"
                                               ng-model="mail2.from"
                                               data-i18n="i18n-placeholder"
                                               required
                                               no-paste
                                               name="contact_email2"
                                               validate-equals="mail.from"
                                               >
                                    </div>
                                    <span ng-show="contact_form.contact_email2.$error.required" data-i18n="id00484_app:contact:obligatorio">Campo obligatorio</span>
                                    <span ng-show="contact_form.contact_email2.$dirty && contact_form.contact_email2.$invalid" data-i18n="id00485_app:contact:emailc">Los campos de email no coinciden o son incorrectos</span>

                                </div>


                                <div class="form-group">
                                    <div class="input-group input-group-lg">
                                        <span class="input-group-addon">
                                            <span class="glyphicon glyphicon-user"></span>
                                        </span>
                                        <input type="text"
                                               class="form-control"
                                               name="contact_name"
                                               placeholder="id00064_app:contact:name"
                                               ng-model="mail.name"
                                               data-i18n="i18n-placeholder"
                                               required
                                               >
                                         
                                    </div>
                                    <span ng-show="contact_form.contact_name.$error.required" data-i18n="id00484_app:contact:obligatorio">Campo obligatorio</span>
                                </div>
                                <div class="form-group">
                                    <div class="input-group input-group-lg">
                                        <span class="input-group-addon">
                                            <span class="glyphicon glyphicon-asterisk"></span>
                                        </span>
                                        <input type="text"
                                               class="form-control"
                                               placeholder="id00066_app:contact:subject"
                                               ng-model="mail.subject"
                                               data-i18n="i18n-placeholder"
                                               >
                                    </div>
                                </div>
                                <div class="form-group" style="text-align:left;">
                                    <!-- Editor -->
                                    <section class="panel panel-default">
                                        <div class="panel-body">
                                            <!--div text-angular ng-model="mail.body" class="ui-editor"></div-->
                                            <textarea text-angular ng-model="mail.body" class="form-control" rows="14"></textarea>
                                        </div>
                                    </section>
                                    <!-- end Editor -->
                                </div>

                                <div class="form-group">
                                    <div
                                         vc-recaptcha
                                         key="model.key"
                                         data-ng-model="mail.recaptcha"
                                         handle-error="reCaptcha"
                                         lang ="es"
                                         ></div>
                                </div>

                            </fieldset>
                        </form>
                    </section>


                </div>

            </div><!-- /container -->

        </div>

        <div id="footer"> 

            <div class="container">
                <div class="row">

                    <div class="col-xs-12">

                        <div class="footer_links_groups_title">
                            <div class="enlaces">
                                <div class="agrupacion">
                                    <ul class="list-inline">
                                        <li><a href="#/app/inicio" data-i18n="id00068_home:footer:home">Inicio</a></li>
                                        <li><a href="#/app/contactar" data-i18n="id00069_home:footer:contact">Contactar</a></li>
                                        <li><a href="#/app/requisitos" data-i18n="id00070_home:footer:requirements">Requisitos</a></li>
                                        <li data-i18n="id00419_home:document:rules"></li>
                                    </ul>
                                   <p data-i18n="id00416_home:footer:title">Plataformas Electrónicas</p>
                                </div>
                            </div>


                        </div>


                    </div>


                </div>
            </div>

        </div> <!-- /footer -->


    </div>

{%endverbatim%}