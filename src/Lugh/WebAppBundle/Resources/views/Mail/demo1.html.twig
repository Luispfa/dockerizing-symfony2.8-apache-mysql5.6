                            <tbody>
                                <tr>
                                    <td class="cell" width="600">
                                        <table class="table" border="0" width="600" cellspacing="0" cellpadding="0">
                                            <tbody>
                                                <tr>
                                                    <td class="logocell" bgcolor="#ffffff" width="250"><img style="display: block; -ms-interpolation-mode: bicubic;" src="{{ app.request.getSchemeAndHttpHost() }}/{{ app.request.getBasePath() }}/bundles/lugh/workspace_base/logo.png" alt="Plantilla email"></td>
                                                    <td class="hide" style="color: #a6a6a6; font-size: 14px; font-family: 'Helvetica Neue',Helvetica,Arial,sans-serif;" align="center" valign="middle" bgcolor="#ffffff" width="350"><br> <strong>Plataformas Electr&oacute;nicas</strong></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                        <br>
                                        <table border="0" width="100%" cellspacing="0" cellpadding="0">
                                            <tbody>
                                                <tr>
