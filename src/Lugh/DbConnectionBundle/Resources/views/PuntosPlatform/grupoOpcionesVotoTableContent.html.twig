<table id="crontab_file" class="table table-striped table-bordered responsive">
    <thead>
        <tr>
            <th>Tag</th>
            <th>Grupo Opciones Voto</th>
        </tr>
    </thead>
    
    <tbody>
        {% for opcionvoto_line in opcionesvoto_lines %}
            {% set name = 'name_' ~ opcionvoto_line.id %}
            {% set opcionesvoto = 'opcionesvoto_' ~ opcionvoto_line.id %}
            <input type="hidden" name="id[grupoopcionvoto][]" value="{{ opcionvoto_line.id }}" />
            <tr class ="gradeA">
                <td>
                    {{ form_widget(attribute(opcionvoto_line.name,name)) }}
                </td>
                <td>
                   {{ form_widget(attribute(opcionvoto_line.opcionesvoto,opcionesvoto)) }} 
                </td>
            </tr>
        {% endfor%}
    </tbody>

</table>