<table id="crontab_file" class="table table-striped table-bordered responsive">
    <thead>
    <tr>
        <th>key</th>
        <th>Config</th>

    </tr>
    </thead>

    <tbody>
    <tr class ="gradeA">

        {% for element in parameters_junta %}
            {% set widget = 'juntas_api_' ~ element.id %}
            <input type="hidden" name="id[junta][]" value="{{ element.id }}" />
            <tr>
                <td>
                    {{ element.clase }}
                </td>
                <td>
                    {{ form_widget(attribute(element.config,widget)) }}
                </td>
            </tr>
        {% endfor %}
    </tr>
    </tbody>

</table>
